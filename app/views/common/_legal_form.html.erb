<div class="error-messages"></div>

<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <h6>Company Type</h6>
    </div>
  </div>
</div>

<div class="container">
  <div class="row">
    <div class="col-lg-12 fieldcontainer">
      <%= vl.select(:company_type_id, options_for_select(VendorLegal::COMPANY_TYPE.invert, vl.object.company_type_id), {include_blank: "Company Type"}, class: "form-control") %>
    </div>
  </div>
</div>

<div class="container">
  <div class="row">
    <div class="col-lg-6 fieldcontainer">
      <h6>Incorporation Date</h6>
      <%= vl.date_field :incorporation_date, class: "form-control", placeholder: "Enter Incorporation Date" %>
    </div>
    <div class="col-lg-6 fieldcontainer">
      <h6>CIN</h6>
      <%= vl.text_field :cin, class: "form-control", placeholder: "Enter CIN Number" %>
    </div>
  </div>
</div>

<div class="container">
  <div class="row">
    <div class="col-lg-6 fieldcontainer">
      <h6>GSTN</h6>
      <%= vl.text_field :gstn, class: "form-control", placeholder: "Enter GST Number" %>
    </div>
    <div class="col-lg-6 fieldcontainer">
      <h6>Pan</h6>
      <%= vl.text_field :pan, class: "form-control", placeholder: "Enter Pan Number" %>
    </div>
  </div>
</div>